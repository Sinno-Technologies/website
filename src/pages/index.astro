---
import Layout from "../layouts/Layout.astro"; 
import Header from "../components/Header.astro";
import Hero from "../components/Hero.astro";
import Process from "../components/Process.astro";
import Team from "../components/Team.astro";
import Projects from "../components/Projects.astro";
import GoogleReviews from "../components/googleReviewsCarousel";
import Contact from "../components/Contact.astro";
import Footer from "../components/Footer.astro";
import Calendly from "../components/Calendly.astro";
---

<Layout seo={{
	title: "Sinno Technologies Limited",
	description: "Professional Software Development and Web Accessibility Auditing Services."
}}>
	<Header />
	<main class="wrapper" id="home">
		 <section id="hero">
			<Hero />
		 </section>
		<section class='content'>
			<Process />
			<Calendly />
			<Team />
			<Projects />
			<GoogleReviews client:visible/>
			<Contact />
		</section>
	</main>
	<Footer />
</Layout>

<style>
	#hero{
		width: 100%;
	}
</style>